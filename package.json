{
  "name": "dexs-website",
  "version": "1.0.0",
  "description": "",
  "source": "public/index.html",
  "parcel-namer-rewrite": {
    "chain": "@parcel/namer-default",
    "silent": true,
    "rules": {
      "(.*)\\.(svg|png|gif|jpg)": "images/$1{.hash}.$2",
      "(.*)\\.(ttf|woff|eot|woff2)": "fonts/$1{.hash}.$2",
      "(.*)\\.(js|js.map)": "js/$1{.hash}.$2",
      "(.*)\\.(css)": "styles/$1{.hash}.$2"
    }
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "scripts": {
    "prebuild": "run-s clean copy-static",
    "build": "parcel build --no-source-maps",
    "clean": "rimraf ./dist",
    "lint": "run-s lint:code",
    "lint:code": "eslint --ext ts,tsx src",
    "dev": "parcel",
    "start": "npm-run-all prebuild dev",
    "test": "jest",
    "copy-static": "copyfiles --flat src/assets/static/*.jpg dist/static"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.11.0",
    "@parcel/transformer-inline-string": "^2.11.0",
    "@parcel/transformer-sass": "^2.11.0",
    "@parcel/transformer-svg-react": "^2.11.0",
    "@parcel/transformer-typescript-tsc": "^2.11.0",
    "@parcel/transformer-webmanifest": "^2.11.0",
    "@parcel/utils": "^2.11.0",
    "@reactivex/rxjs": "^6.6.7",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "copyfiles": "^2.4.1",
    "devlop": "^1.1.0",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.11.0",
    "parcel-namer-rewrite": "^2.10.3-rc.2",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "sass": "^1.50.1",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "lit": "^2.2.2",
    "md-block": "^0.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.21.2",
    "rehype-mermaid": "^2.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mermaidjs": "^6.0.0"
  }
}
